{
  "name": "hedgerow",
  "version": "1.0.2",
  "description": "dynamic polymorphism on constrained sequences of variables",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": ["lib/**/*"],
  "scripts": {
    "clean": "rimraf .\\dist",
    "build": "npm run clean && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "test": "jest",
    "version": "npm run format && git add -A src"
  },
  "author": "stratagyn",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/stratagyn/hedgerow#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/stratagyn/hedgerow.git"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}